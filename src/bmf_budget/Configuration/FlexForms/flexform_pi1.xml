                                    <numIndex index="1">
                                        <numIndex index="0">LLL:EXT:bmf_budget/Resources/Private/Language/locallang_flexform_pi1.xlf:strucure_function</numIndex>
                                        <numIndex index="1">group</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">LLL:EXT:bmf_budget/Resources/Private/Language/locallang_flexform_pi1.xlf:strucure_group</numIndex>
                                        <numIndex index="1">function</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.default.structure>

                </el>
